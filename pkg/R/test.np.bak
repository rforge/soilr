test.np=function(){
### This function tests the Euler and the odesolve method against the analytical solution for
### the parallel two pool model 
(){
# We use the twopool parallel model
# We compute a simple analytical solution first
t_start=0 
t_end=10 
tn=10 
tol=.02/tn
print(tol)
timestep=(t_end-t_start)/tn 
times=seq(t_start,t_end,timestep) 
k1=-0.5 
k2=-0.2
c10=1
c20=2
target=TwopParallelAnalytical(times,k1,k2,c10,c20)
# now we compute the solution with the numeric approaches 
Yeuler	=TwopParallel(times,k1,k2,c10,c20,euler)
Yode	=TwopParallel(times,k1,k2,c10,c20,ode)
print(Yode)
print(Yeuler)
##
checkEquals(target, Yode, "test numeric solution computed by ode package against analytical",
         #tolerance = .Machine$double.eps^0.5, 
         tolerance = tol,
)
checkEquals(target, Yeuler, "test numeric solution computed by the euler mehtod against analytical",
         tolerance = tol,
	 )
## We now proceed with the three pool parallel example
k3=-0.5
c30=3
target=ThreepParallelAnalytical(times,k1,k2,k3,c10,c20,c30)
Yeuler=ThreepParallel(times,k1,k2,k3,c10,c20,c30)

checkEquals(target, Yeuler, "test numeric solution computed by the euler mehtod against analytical",
         tolerance = tol,
)
}
